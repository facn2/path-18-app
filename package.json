{
  "name": "path-18-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "devStart": "nodemon server/start_server.js",
    "build": "react-scripts build && npm run move:build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit-test": "CI=true npm test",
    "db-build": "node database/db_build.js",
    "pretest": "ENV=testing npm run db-build",
    "move:build": "rm -r server/assets/static && mv -f build/* server/assets/"
  },
  "pre-commit": ["precommit-test"],
  "dependencies": {
    "axios": "^0.17.1",
    "body-parser": "^1.18.2",
    "env2": "^2.2.0",
    "express": "^4.16.2",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "handlebars": "^4.0.11",
    "jsonwebtoken": "^8.1.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "pg": "^7.4.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-slick": "^0.16.0",
    "react-swing": "^1.0.2",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.2.3",
    "swing": "^3.1.1"
  },
  "devDependencies": {
    "nodemon": "^1.12.1",
    "pre-commit": "^1.2.2"
  }
}
